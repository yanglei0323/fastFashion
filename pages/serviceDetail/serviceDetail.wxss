@import '../../styles/mycell.wxss';
.cell-box .cell-manage::after{
    display:none;
}
.cell-box .cell-manage{
    padding-left:20rpx;
}
.mt-10{
    margin-top:10rpx;
}
.container{
    background:#ececec;
    padding-bottom:200rpx;
}
.bg-fff{
    background:#fff;
}
.service-title{
    height:208rpx;
    width:100%;
    display:flex;
    flex-direction:column;
    padding:0 20rpx;
}
.mt-20{
    margin-top:20rpx;
}
.mt-10{
    margin-top:10rpx;
}
.mt-15{
    margin-top:15rpx;
}
.mt-60{
    margin-top:60rpx;
}
.ml-20{
    margin-left:20rpx;
}
.pl-108{
    padding-left:108rpx;
}
.item-title{
    font-size:28rpx;
    line-height:28rpx;
    color:#333;
}
.item-cont{
    font-size:20rpx;
    line-height:20rpx;
    color:#999;
}
.through{
    text-decoration:line-through;
}
.service-cell{
    height:132rpx;
    padding-right:20rpx;
    overflow:hidden;
    text-overflow:ellipsis;
    white-space:nowrap;
    font-size:20rpx;
    line-height:132rpx;
    position:relative;
    border-bottom:1rpx solid #ececec;
}
.service-icon{
    width:62rpx;
    height:62rpx;
    position:absolute;
    left:26rpx;
    top:35rpx;
}
.info-msg{
    font-size:24rpx;
    padding:12rpx 20rpx;
    line-height:60rpx;
}
.fixedbtn-box{
    width:100%;
    height:88rpx;
    position:fixed;
    left:0;
    bottom:0;
    line-height:88rpx;
    font-size:28rpx;
    display:flex;
    text-align:center;
    justify-content:space-between;
}
.fixedbtn-left{
    width:49%;
}
.fixedbtn-right{
    color:#fff;
    width:50%;
}