.container{
	background:#ececec;
	padding-bottom:100rpx;
}
.coupon-cont{
	width:730rpx;
	height:200rpx;
	margin:10rpx auto 0;
	position:relative;
}
.coupon-bg{
	width:100%;
	height:100%;
	position:absolute;
	left:0;
	top:0;
	z-index:99;
}
.coupon-left{
	width:240rpx;
	height:100%;
	display:flex;
	flex-direction:column;
	justify-content:center;
	align-items:center;
	position:absolute;
	left:0;
	top:0;
	z-index:999;
	color:#fff;
	font-size:64rpx;
}
.coupon-cond{
	font-size:24rpx;
}
.coupon-right{
	width:440rpx;
	display:flex;
	flex-direction:column;
	position:absolute;
	left:240rpx;
	top:0;
	z-index:999;
	color:#666;
	font-size:20rpx;
	border-bottom:1rpx dashed #ececec;
	padding:20rpx 20rpx 10rpx;
}
.coupon-lab{
	display:inline-block;
	width:76rpx;
	height:30rpx;
	background:#f6838d;
	color:#fff;
	line-height:30rpx;
	text-align:center;
	margin-right:10rpx;
	border-radius:2rpx;
}
.coupon-type{
	font-size:24rpx;
	line-height:24rpx;
	margin:42rpx 0 30rpx 0;
}
.coupon-time{
	font-size:20rpx;
	line-height:20rpx;
	color:#bcbcbc;
}
.coupon-select{
	position:absolute;
	right:75rpx;
	top:92rpx;
	z-index:999;
}
.fixedbtn-box{
	width:100%;
	height:88rpx;
	position:fixed;
	left:0;
	bottom:0;
	font-size:28rpx;
	color:#333;
	line-height:88rpx;
	text-align:center;
	z-index:999;
}