.store-md{
    width: 100%;
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    align-items: flex-start;
    flex-wrap: wrap;
    margin-top:62rpx;
    padding:0 20rpx;
}
.store-md-title{
    font-size:36rpx;
    color:#333;
    line-height:36rpx;
}
.store-md-msg{
    width:100%;
    font-size:20rpx;
    color:#999;
    line-height:20rpx;
    margin-top:10rpx;
    display: flex;
    justify-content: space-between;
}
.store-list{
    padding:30rpx 20rpx;
    border-bottom:1rpx solid #ececec;
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    align-items: flex-start;
    flex-wrap: wrap;
}
.store-img{
    width:100%;
    height:375rpx;
}
.store-title{
    font-size:28rpx;
    color:#333;
    line-height:28rpx;
    margin-top:30rpx;
}